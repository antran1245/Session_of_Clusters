{
  "manifest_version": 3,
  "name": "Session of Clusters(beta)",
  "version": "0.0.0.6",
  "description": "A Chrome extension built to manage tabs by allowing users to save them for another time.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "windows",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/SessionOfCluster_16.png",
    "32": "icons/SessionOfCluster_32.png",
    "48": "icons/SessionOfCluster_48.png",
    "128": "icons/SessionOfCluster_128.png"
  }
}