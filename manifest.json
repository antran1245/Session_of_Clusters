{
  "manifest_version": 3,
  "name": "Tabmark",
  "version": "1.0.0",
  "description": "A Chrome extension built to manage tabs by saving them for another time.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "windows",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}